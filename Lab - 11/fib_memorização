def fibonacci(n, memo=None):
    if memo is None:
        memo = {}
    
    if n in memo:
        return memo[n]
    
    if n == 0:
        memo[n] = 0
    elif n == 1:
        memo[n] = 1
    else:
        memo[n] = fibonacci(n - 1, memo) + fibonacci(n - 2, memo)
    
    return memo[n]

n = int(input("Digite a posição da sequência de Fibonacci: "))
print(f"Fibonacci na posição {n} (recursividade com memorização): {fibonacci(n)}")
